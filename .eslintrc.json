{
    "extends": "airbnb-base",
    "env": {
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "object-shorthand": ["error", "never"],
        "no-underscore-dangle": ["error", { "allow": ["_id"] }],
        "radix": ["error", "as-needed"]
    }
}
